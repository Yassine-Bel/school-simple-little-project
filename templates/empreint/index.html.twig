{% extends 'base.html.twig' %}

{% block title %}Empreint index{% endblock %}

{% block body %}
    <h1>Empreint index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for empreint in empreints %}
            <tr>
                <td>{{ empreint.id }}</td>
                <td>
                    <a href="{{ path('app_empreint_show', {'id': empreint.id}) }}">show</a>
                    <a href="{{ path('app_empreint_edit', {'id': empreint.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_empreint_new') }}">Create new</a>
{% endblock %}
